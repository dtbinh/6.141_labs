<launch>
  <include file="$(find lab4)/launch/autonomous-config.launch" />

  <node name="rep" pkg="image_transport" type="republish" args="compressed in:=/gscam/image_raw raw out:=/rss/video" machine="netbook" output="screen" />
  <node name="view" pkg="image_view" type="image_view" args="image:=/rss/video" machine="netbook" output="screen" />
  <node name="gui" pkg="rosjava_bootstrap" type="run.py" args="lab4 VisualServo.VisualServo" machine="netbook" output="screen" />
  <node name="lis" pkg="rosjava_bootstrap" type="run.py" args="uorc_listener Listener" machine="netbook" output="screen" /> 
  <node name="pub" pkg="rosjava_bootstrap" type="run.py" args="uorc_publisher Publisher" machine="netbook" output="screen" />
  <node name="odo" pkg="rosjava_bootstrap" type="run.py" args="odometry odometry.Odometry" machine="netbook" output="screen" /> 
  <node name="vid" pkg="gscam" type="gscam" machine="netbook" /> 
</launch>
